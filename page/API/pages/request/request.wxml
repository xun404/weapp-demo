<import src="../../../common/head.wxml"></import>
<import src="../../../common/foot.wxml"></import>
<view class="container">
    <template is="head" data="{{title:'request'}}"></template>
    <view class="page-body">
        <view class="page-body-wording">
            <text class="page-body-text">
        点击向服务器发起请求
      </text>
        </view>
        <view class="btn-area">
            <button bindtap="makeRequest" disabled="{{buttonDisabled}}" loading="{{loading}}" type="primary">request</button>
        </view>
    </view>
    <template is="foot" data></template>
</view>
